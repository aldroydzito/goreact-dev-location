{
  "parser": "babel-eslint",
  "extends": "standard",
  "plugins": ["react"],
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": [2],
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx"] }
    ],
    "import/prefer-default-export": "off"
  }
}
